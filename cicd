pipeline {
    agent any

    stages {
        stage('git pull') {
            steps {
                git 'https://github.com/Pritam-Khergade/student-ui.git'
            }
        }
        stage('awscli install') {
            steps {
                sh 'sudo apt-get update -y '
                sh 'sudo apt install awscli -y '
            }
        }
    }
}    